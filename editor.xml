<application>
  <component name="CodeInsightSettings">
    <option name="AUTO_POPUP_JAVADOC_INFO" value="true" />
    <EXCLUDED_PACKAGE NAME="android.view.View" />
    <EXCLUDED_PACKAGE NAME="ch.publisheria.bring.R.*" />
    <EXCLUDED_PACKAGE NAME="ch.publisheria.bring.lib.model.ImmutableBringMigrateToSecureApiResult.of" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="15" />
    <option name="FONT_FAMILY" value="Droid Sans Mono" />
    <option name="FONT_SCALE" value="1.25" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="ADDITIONAL_PAGE_AT_BOTTOM" value="true" />
  </component>
  <component name="JetEditorOptions">
    <option name="donTShowConversionDialog" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="important" pattern=".*multipleitems.*" />
    <pattern icon="question" pattern="\btodo\b.*BRING-\d+" />
    <filter name="Multiitem">
      <pattern index="2" />
    </filter>
    <filter name="StoryRelated">
      <pattern index="3" />
    </filter>
  </component>
</application>